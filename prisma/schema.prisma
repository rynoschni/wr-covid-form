generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Form {
  id            Int      @id @default(autoincrement())
  project       String   @default("What Remains") @db.VarChar(255)
  question1     String?
  question2     String?
  question3     String?
  question4     String?
  question5     String?
  q5Location    String?
  signature1    String?
  signature1box Boolean? @default(false)
  createdAt     DateTime @default(now()) @db.Timestamp(6)
  userID        Int
  temp          String?  @db.VarChar
  results       String?
  User          User     @relation(fields: [userID], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Form_authorId_fkey")
}

model User {
  id          Int       @id @default(autoincrement())
  email       String    @unique @db.VarChar(255)
  fname       String?   @db.VarChar(255)
  lname       String?   @db.VarChar(255)
  address1    String?   @db.VarChar(255)
  address2    String?   @db.VarChar(255)
  city        String?
  state       String?
  zip         String?   @db.VarChar
  phone       String?   @db.VarChar(255)
  race        String?   @db.VarChar(255)
  ethnicity   String?   @db.VarChar(255)
  password    String?   @db.VarChar
  isAdmin     Boolean?  @default(false)
  gender      String?
  county      String?
  dob         DateTime? @db.Date
  ecFirstName String?
  ecLastName  String?
  ecPhone     String?
  ecRelation  String?
  Form        Form[]
}
